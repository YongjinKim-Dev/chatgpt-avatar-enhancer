{
  "name": "ChatGPT Avatar Enhancer",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Add avatars to ChatGPT conversations via a Chrome Extension",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "options_page": "options.html"
}

